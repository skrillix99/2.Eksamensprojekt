@page
@model _2.Eksamensprojekt.Pages.AdministrationPages.AdministrationBookingModel
@{
}

<h1>booking system til administration</h1>

<form action="/" method="post">
    <br />
    <div class="row">
        <div class="col-md-12">
            <h6>Datetimepicker</h6>

            <div class="form-group">
                <div class="input-group date" id="datetimepicker">
                    <asp:Calendar ID="Calendar1" TodayDayStyle-Font-Underline="false" runat="server" SelectionMode="None" OnDayRender="Calendar1_DayRender">
                        <DayStyle Font-Underline="False" Wrap="False" />
                    </asp:Calendar>
                </div>
            </div>
        </div>

        <script>
                    protected void Calendar1_DayRender(object sender, DayRenderEventArgs e)
                {
                    if (e.Day.Date < DateTime.Now.Date) {
                        e.Day.IsSelectable = false;
                        e.Cell.ForeColor = System.Drawing.Color.Gray;
                    }
                }
        </script>
    </div>
    @*<input type="datetime-local" value="welp2"/>*@
</form>

